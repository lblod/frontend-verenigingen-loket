{{#if @association}}
  {{#if this.getCurrentRecognitionStatus.isFinished}}
    {{#if (eq this.getCurrentRecognitionStatus.value "Erkend")}}
      <AuPill @skin="success">Erkend</AuPill>
    {{else if (eq this.getCurrentRecognitionStatus.value "Verlopen")}}
      <AuPill @skin="warning">Verlopen</AuPill>
    {{/if}}
  {{/if}}
{{/if}}